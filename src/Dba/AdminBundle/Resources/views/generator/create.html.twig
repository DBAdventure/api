{% extends 'DbaAdminBundle::base.html.twig' %}

{% block body %}
  <h1>{{ 'admin.map.create' | trans }}</h1>

  <form id="register-form" class="form-horizontal" method="post" action="{{ path('admin.generator.map.create') }}" autocomplte="off">
    {{ form_row(form.name) }}
    {{ form_row(form.maxX) }}
    {{ form_row(form.maxY) }}
    {{ form_widget(form._token) }}

    <div class="col-sm-offset-2 col-sm-10">
      <button type="submit" class="btn btn-default">{{ 'save' | trans }}</button>
    </div>
  </form>
{% endblock %}
